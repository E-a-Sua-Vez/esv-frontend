{
  "name": "ett-frontend",
  "version": "0.0.0",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --config vite.config-net.js",
    "dev:br": "bash -c 'source env_br.sh && vite --config vite.config-br.js'",
    "build:net": "vite --config vite.config-net.js build --base=/",
    "build:br": "vite --config vite.config-br.js build --base=/",
    "build:testbr": "vite --config vite.config-test-br.js build --base=/",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{js,jsx,vue,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,vue,ts,tsx,json,css,scss,md}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "serve": "vue-cli-service serve",
    "create-env": "printenv > .env",
    "watch": "vue-cli-service build --watch --mode development"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "@popperjs/core": "^2.11.8",
    "@vueform/toggle": "^2.1.3",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "bootstrap": "^5.2.2",
    "bootstrap-icons": "^1.10.2",
    "chart.js": "^3.9.1",
    "dayjs": "^1.8.27",
    "firebase": "^11.10.0",
    "firebase-tools": "^13.0.0",
    "html2pdf.js": "^0.10.1",
    "jspdf": "^3.0.4",
    "pinia": "^2.0.23",
    "qrcode.vue": "^3.3.3",
    "v-calendar": "^3.1.2",
    "vue": "^3.2.41",
    "vue-chart-3": "^3.1.8",
    "vue-gtag": "^2.1.2",
    "vue-i18n": "^9.14.5",
    "vue-recaptcha": "^2.0.3",
    "vue-router": "^4.1.5",
    "vue3-google-map": "0.20.0",
    "vue3-popper": "^1.5.0",
    "vue3-star-ratings": "1.1.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "happy-dom": "^20.0.10",
    "husky": "^8.0.3",
    "jsdom": "^20.0.1",
    "lint-staged": "^15.5.2",
    "prettier": "^2.7.1",
    "vite": "^7.2.4",
    "vitest": "^1.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix --max-warnings=9999",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
